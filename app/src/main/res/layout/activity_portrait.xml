        </LinearLayout>

        <LinearLayout
            android:layout_width="389dp"
            android:layout_height="126dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="334dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa4" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="334dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa5" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="334dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa6" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="120dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="305dp"
                android:layout_height="107dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa9" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="305dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa7" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="305dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa8" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="305dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/pa10" />
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="SendPortrait_SaveChange"
        android:text="Save Change"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.500"
        app:layout_constraintVertical_bias="0.850"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="662dp" />

</androidx.constraintlayout.widget.ConstraintLayout>